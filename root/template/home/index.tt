<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
<link type="text/css" href=  "[% c.uri_for('/src/template.css')%]" rel="Stylesheet" />
<link href="http://code.google.com/apis/maps/documentation/javascript/examples/default.css" rel="stylesheet" type="text/css" />
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
<script type="text/javascript">
var map;
var map2;
var map_shop;
var map_night;
function initialize() {
  var myLatlng = new google.maps.LatLng(13.820688,100.664889);
  var myOptions = {
    zoom: 10,
    center: myLatlng,
    mapTypeId: google.maps.MapTypeId.ROADMAP
  }
  map = new google.maps.Map(document.getElementById("map_canvas_rest"), myOptions);
  map2 = new google.maps.Map(document.getElementById("map_canvas2"), myOptions);
  map_shop = new google.maps.Map(document.getElementById("map_canvas_shop"), myOptions);
  map_night = new google.maps.Map(document.getElementById("map_canvas_night"), myOptions);
  
}

$(document).ready(function(){
initialize();
[% WHILE (restaurant = restaurants.next)%]
    var la = [%-restaurant.la-%];
    var llong = [%-restaurant.long -%];
    var myLatlng = new google.maps.LatLng(la,llong);<link type="text/css" href=  "[% c.uri_for('/src/template.css')%]" rel="Stylesheet" />
    var marker = new google.maps.Marker({
        position: myLatlng,
        title:"[%restaurant.placename%]"
    });
    // To add the marker to the map, call setMap();

    marker.setMap(map); 
[% END%]
});

$(document).ready(function(){
	[% WHILE (hotel = hotels.next)%]
	    var la = [%-hotel.la-%];
	    var llong = [%-hotel.long -%];
	    var myLatlng = new google.maps.LatLng(la,llong);
	    var marker = new google.maps.Marker({
	        position: myLatlng,
	        title:"[%hotel.placename%]"
	    });
	    // To add the marker to the map, call setMap();

	    marker.setMap(map2); 
	[% END%]
	});

$(document).ready(function(){
    [% WHILE (shop = shoppings.next)%]
        var la = [%-shop.la-%];
        var llong = [%-shop.long -%];
        var myLatlng = new google.maps.LatLng(la,llong);
        var marker = new google.maps.Marker({
            position: myLatlng,
            title:"[%shop.placename%]"
        });
        // To add the marker to the map, call setMap();

        marker.setMap(map_shop); 
    [% END%]
    });
    
    
$(document).ready(function(){
    [% WHILE (night = nightlifes.next)%]
        var la = [%-night.la-%];
        var llong = [%-night.long -%];
        var myLatlng = new google.maps.LatLng(la,llong);
        var marker = new google.maps.Marker({
            position: myLatlng,
            title:"[%night.placename%]"
        });
        // To add the marker to the map, call setMap();

        marker.setMap(map_night); 
    [% END%]
    });
</script>
</head>
<body>

[% CALL restaurants.reset %]
<div class="bodyframe redmond  ui-widget-content ui-corner-all">
   <div >
        <div id="map_canvas_rest" class="body_inner"></div>
   </div>
    <table border="0" class="table">
        <tr>
            <td></td>
            <td></td>
            <td></td>
        </tr>
        <tr>
            <td>
            </td>
        </tr>
        <tr>
            <td>
                <label>
                    Category : Restaurant
                </label>
            </td>
        </tr>
    </table> 
    [%WHILE (restaurant = restaurants.next)%]
    <div   class = "div_s redmond ui-widget-content ui-corner-all" >
    <table  class="redmond ui-accordion-content ui-helper-reset  ui-accordion-content-active">
        <tr>
            <td>
                <label>
                    Business Name
                </label>
                    <a href="/user/search?place_id=[%restaurant.place_id%]">
                        [%restaurant.placename%]
                    </a>
            </td>
        </tr>
        <tr>
            <td>
                <label>
                    Rate :
                </label>
                    <div>
                        [%i =0;%]
                        [%WHILE i < restaurant.rate%]
                            <span  class="ui-icon ui-icon-star star_icon" ></span>
                            [%i = i+1%]
                        [%END%]
                    </div>
            </td>
        <tr>
            <td >
                <label>
                    [%restaurant.count%] 
                    [%IF(restaurant.count)<= 1%]
                        <label >
                            review
                        </label>
                    [%ELSE%]
                        <label >
                            reviews
                        </label>
                    [%END%]
                </label>
            </td>
            </tr>
        </tr>
        <tr>
            <td></2td>
            <td></td>
            <td></td>
        </tr>
    </table> 
   </div>
    [%END%]
    <a href="/place/show?type_id=4">more>></a>
</div>   




