<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="initial-scale=1.0, user-scalable=no" />

<link href="http://code.google.com/apis/maps/documentation/javascript/examples/default.css" rel="stylesheet" type="text/css" />
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
<script type="text/javascript">
var map;
var la = [%-place.la-%];
var llong = [%-place.long -%];
var centermyLatlng = new google.maps.LatLng(la,llong);

function initialize() {
  var myLatlng = new google.maps.LatLng(13.820688,100.664889);
  var myOptions = {
    zoom: 16,
    center: centermyLatlng,
    mapTypeId: google.maps.MapTypeId.ROADMAP
  }
  map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);
}
  
function placeMarker(location) {
  var clickedLocation = new google.maps.LatLng(location);
  var marker = new google.maps.Marker({
      position: location, 
      map: map
  });

  map.setCenter(location);
}
$(document).ready(function(){
    initialize();
    var input = $("body input[name='latLng']"),
    input_lat = $("body input[name='lat']"),
    input_lng = $("body input[name='lng']");
    
    var get_position = new google.maps.LatLng();
    var la = [%-place.la-%];
    var llong = [%-place.long -%];
    var myLatlng = new google.maps.LatLng(la,llong);
    var marker = new google.maps.Marker({
        position: myLatlng,
        title:"[%place.placename%]"
    });
    
    // To add the marker to the map, call setMap();
    marker.setMap(map); 
    
});
</script>
<style>
#bodyframe{
    margin-top: 20px; 
    font-size: 12px; 
    color: #222222;
}
#table{
    margin-top: 20px;
}
#label_s{
    font-size: 15px;
    color: #222222";

}
#div_s{
    width: 810px;
    margin: 10pt auto;
    border: 1px solid #3BB9FF;"
}
#map_canvas{
    width: 400px; 
    height: 300px; 
    position: relative; 
}
.star_icon {
    float: left;
}
</style>

</head>
<body>
<div id="bodyframe" class="redmond  ui-widget-content ui-corner-all">
   <div >
    <div id="map_canvas" class="body_inner"></div>
   </div>
    <table border="0" id="table">
        <tr>
            <td></td>
            <td></td>
            <td></td>
        </tr>
        <tr>
            <td>
            </td>
        </tr>
        <tr>
            <td>
                <label id="label_s">
                    Business Name : [%place.placename%]
                </label>
            </td>
        </tr>
        <tr>
            <td>
                <label id="label_s";>
                    Location : [%place.location%]
                </label>
            </td>
        </tr>
            <td>
                <label id="label_s";>
                    Rate:[% place.rate %]
                </label>
            </td>
        </tr>
    </table> 
    [%FOREACH review IN place.review%]
    <div  id="div_s" class = "redmond ui-widget-content ui-corner-all" >
    <table  class="redmond ui-accordion-content ui-helper-reset  ui-accordion-content-active">
        <tr>
            <td>
                <label>
                    Rate :
                </label>
                <div>
                [%i =0;%]
                [%WHILE i < review.rate%]
                <span  class="ui-icon ui-icon-star star_icon" ></span>
                    [%i = i+1%]
                [%END%]
                </div>
            </td>
            <tr>
            <td >
                <label>
                    Review:
                </label>
                <label>
                    [%review.detail%]
                </label>
            </td>
            </tr>
            <td>
                <label>
                    By:
                    [% review.user.firstname %]
            </td>
        </tr>
        <tr>
            <td></td>
            <td></td>
            <td></td>
        </tr>
    </table> 
   </div>
    [%END%]
</div>   
</body>
</html>